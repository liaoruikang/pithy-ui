{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "ESNext", "DOM.Iterable"],
    "sourceMap": false,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "allowImportingTsExtensions": true
  }
}
